<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE dwr PUBLIC 
	"-//GetAhead Limited//DTD Direct Web Remoting 3.0//EN"
	"http://getahead.org/dwr/dwr30.dtd">
<dwr>
	<allow>
		<create creator="spring" javascript="AdminManager"> 
			<param name="beanName" value="adminManager"/>
		</create>
		<create creator="spring" javascript="RoomManager"> 
			<param name="beanName" value="roomManager"/>
		</create>
		<create creator="spring" javascript="PhotoManager"> 
			<param name="beanName" value="photoManager"/>
		</create>
		<create creator="spring" javascript="UserManager"> 
			<param name="beanName" value="userManager"/>
		</create>
		<create creator="spring" javascript="BookingManager"> 
			<param name="beanName" value="bookingManager"/>
		</create>
		<create creator="spring" javascript="CommentManager"> 
			<param name="beanName" value="commentManager"/>
		</create>
		<create creator="spring" javascript="MessageManager"> 
			<param name="beanName" value="messageManager"/>
		</create>
		<!-- 定义使用Bean转换器处理如下Java类 -->
		<convert converter="bean" match="com.xwkj.booking.bean.RoomBean"/>
		<convert converter="bean" match="com.xwkj.booking.bean.PhotoBean"/>
		<convert converter="bean" match="com.xwkj.booking.bean.UserBean"/>
		<convert converter="bean" match="com.xwkj.booking.bean.BookingBean"/>
		<convert converter="bean" match="com.xwkj.booking.bean.CommentBean"/>
		<convert converter="bean" match="com.xwkj.booking.bean.MessageBean"/>
	</allow>
</dwr>